{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "darkaonline/l5-swagger": "^8.3.3",
        "guzzlehttp/guzzle": "^7.4.5",
        "laravel/framework": "^9.25.1",
        "laravel/sanctum": "^2.15.1",
        "laravel/tinker": "^2.7.2",
        "pusher/pusher-php-server": "^7.0.2",
        "tcg/voyager": "^1.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "fakerphp/faker": "^1.20",
        "laravel/pint": "^1.1.1",
        "laravel/sail": "^1.15.4",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.2.1",
        "nunomaduro/larastan": "^2.1.12",
        "phpunit/phpunit": "^9.5.23",
        "spatie/laravel-ignition": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "larastan": "./vendor/bin/phpstan analyse",
        "generate-api-doc": "@php artisan l5-swagger:generate",
        "pint": "./vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
